<?xml version="1.0" encoding="utf-8"?> <!--    DAY LA FRAME 5-->
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray"
    tools:context=".MenuChoiceActivity">

    <include layout="@layout/toolbar_blue_frame_5"
        android:id="@+id/tool"
        />

    <Button
        android:id="@+id/settings"
        android:layout_width="fill_parent"
        android:layout_height="100dp"
        android:layout_marginTop="70dp"
        android:drawableLeft="@drawable/ic_setting"
        android:drawablePadding="-140dp"
        android:text="Settings"
        android:textColor="@color/black"
        android:textSize="30dp"
        android:paddingLeft="10dp"
        android:gravity="center"
        android:background="@color/white"
        android:textAllCaps="false"
        tools:ignore="RtlCompat" />

    <Button
        android:id="@+id/helps"
        android:layout_width="fill_parent"
        android:layout_height="100dp"
        android:layout_marginTop="190dp"
        android:drawableLeft="@drawable/ic_help"
        android:drawablePadding="-180dp"
        android:text="Helps"
        android:textColor="@color/black"
        android:textSize="30dp"
        android:textAllCaps="false"
        android:paddingLeft="10dp"
        android:gravity="center"
        android:background="@color/white"
        tools:ignore="RtlCompat" />

    <Button
        android:id="@+id/total_nutritions"
        android:layout_width="fill_parent"
        android:layout_height="100dp"
        android:layout_marginTop="310dp"
        android:drawableLeft="@drawable/ic_total"
        android:drawablePadding="-50dp"
        android:text="Total Nutritions"
        android:textColor="@color/black"
        android:textSize="30dp"
        android:paddingLeft="10dp"
        android:textAllCaps="false"
        android:gravity="center"
        android:background="@color/white"
        tools:ignore="RtlCompat" />

    <Button
        android:id="@+id/logout"
        android:layout_width="fill_parent"
        android:layout_height="100dp"
        android:layout_marginTop="625dp"
        android:drawableLeft="@drawable/ic_logout"
        android:drawablePadding="-150dp"
        android:text="Log Out"
        android:textColor="@color/black"
        android:textSize="30dp"
        android:paddingLeft="10dp"
        android:textAllCaps="false"
        android:gravity="center"
        android:background="@color/white"
        tools:ignore="RtlCompat" />

</RelativeLayout>
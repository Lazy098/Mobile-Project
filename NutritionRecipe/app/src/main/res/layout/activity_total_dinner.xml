<?xml version="1.0" encoding="utf-8"?> <!--    DAY LA FRAME 10 NHUNG THAY BREAKFAST = DINNER-->
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray"
    tools:context=".TotalDinnerActivity">

    <include layout="@layout/toolbar_total_dinner"
        android:id="@+id/tool"
        />

    <TextView
        android:id="@+id/text_calories_dinner"
        android:layout_marginTop="70dp"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:text="Calories"
        android:textSize="16dp"
        android:paddingLeft="10dp"
        android:textColor="@color/black"
        android:gravity="center_vertical"
        android:background="@color/white"/>

    <TextView
        android:id="@+id/text_fat_dinner"
        android:layout_marginTop="140dp"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:text="Fat"
        android:textSize="16dp"
        android:paddingLeft="10dp"
        android:textColor="@color/black"
        android:gravity="center_vertical"
        android:background="@color/white"/>

    <TextView
        android:id="@+id/text_saturated_dinner"
        android:layout_marginTop="210dp"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:text="Saturated"
        android:textSize="16dp"
        android:paddingLeft="10dp"
        android:textColor="@color/black"
        android:gravity="center_vertical"
        android:background="@color/white"/>

    <TextView
        android:id="@+id/text_polyunsaturated_dinner"
        android:layout_marginTop="280dp"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:text="Polyunsaturated"
        android:textSize="16dp"
        android:paddingLeft="10dp"
        android:textColor="@color/black"
        android:gravity="center_vertical"
        android:background="@color/white"/>

    <TextView
        android:id="@+id/text_monounsaturated_dinner"
        android:layout_marginTop="350dp"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:text="Monounsaturated"
        android:textSize="16dp"
        android:paddingLeft="10dp"
        android:textColor="@color/black"
        android:gravity="center_vertical"
        android:background="@color/white"/>

    <TextView
        android:id="@+id/text_amount_calories_dinner"
        android:layout_marginTop="70dp"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:text="0 calo"
        android:textSize="16dp"
        android:textColor="@color/black"
        android:padding="10dp"
        android:gravity="right|center_vertical"
        android:layout_marginLeft="250dp"
        android:background="@color/white"/>

    <TextView
        android:id="@+id/text_amount_fat_dinner"
        android:layout_marginTop="140dp"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:text="0 g     "
        android:textSize="16dp"
        android:textColor="@color/black"
        android:gravity="right|center_vertical"
        android:layout_marginLeft="250dp"
        android:padding="10dp"
        android:background="@color/white"/>

    <TextView
        android:id="@+id/text_amount_saturated_dinner"
        android:layout_marginTop="210dp"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:text="0 g     "
        android:textSize="16dp"
        android:padding="10dp"
        android:textColor="@color/black"
        android:gravity="right|center_vertical"
        android:layout_marginLeft="250dp"
        android:background="@color/white"/>

    <TextView
        android:id="@+id/text_amount_polyunsaturated_dinner"
        android:layout_marginTop="280dp"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:text="0 g     "
        android:textSize="16dp"
        android:textColor="@color/black"
        android:gravity="right|center_vertical"
        android:padding="10dp"
        android:layout_marginLeft="250dp"
        android:background="@color/white"/>

    <TextView
        android:id="@+id/text_amount_monosaturated_dinner"
        android:layout_marginTop="350dp"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:text="0 g     "
        android:textSize="16dp"
        android:textColor="@color/black"
        android:gravity="right|center_vertical"
        android:padding="10dp"
        android:layout_marginLeft="250dp"
        android:background="@color/white"/>

</RelativeLayout>